{
  "name": "scrap",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:front": "vite dev --port 8010",
    "dev:back": "bun --watch server.js",
    "build": "vite build",
    "preview": "vite preview --port 8010 --host 0.0.0.0 & bun server.js",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.8.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/bun": "latest",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.8",
    "svelte": "^5.1.16",
    "svelte-check": "^4.0.7",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  },
  "dependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.26.0",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/adapter-static": "^3.0.6",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "flowbite": "^2.5.2",
    "flowbite-svelte": "^0.47.3",
    "flowbite-svelte-icons": "^2.0.2",
    "openai": "^4.72.0",
    "postcss": "^8.4.49",
    "svelte-persisted-store": "^0.12.0",
    "tailwindcss": "^3.4.14"
  },
  "module": "index.ts"
}
